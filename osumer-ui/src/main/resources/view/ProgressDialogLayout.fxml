<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>


<DialogPane prefHeight="116.0" prefWidth="323.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="com.github.mob41.osumer.ui.ProgressDialogController">
   <header>
      <Label fx:id="headerText" text="In Progress...">
         <font>
            <Font size="16.0" />
         </font>
         <padding>
            <Insets bottom="8.0" left="16.0" right="8.0" top="8.0" />
         </padding>
      </Label>
   </header>
   <content>
      <BorderPane>
         <bottom>
            <ProgressBar fx:id="progressBar" prefHeight="18.0" prefWidth="291.0" progress="0.0" BorderPane.alignment="CENTER" />
         </bottom>
         <center>
            <Label fx:id="statusText" text="Status:" BorderPane.alignment="CENTER_LEFT">
               <BorderPane.margin>
                  <Insets />
               </BorderPane.margin>
            </Label>
         </center>
      </BorderPane>
   </content>
</DialogPane>
